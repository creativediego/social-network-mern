{
  "name": "social-network-node-api",
  "version": "1.0.0",
   "engines": {
        "node": "18.x"
      },
  "description": "Social Network API built in Node, Mongo, Express, and Typescript.",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "cd client && npm install && npm run build",
    "install-client": "cd .. && cd client && npm install && npm run build",
    "start": "node build/server.js",
    "dev": "nodemon -L ./server.ts",
    "build": "tsc --project ./",
    "postbuild": "npm run install-client",
    "test": "jest --watchAll --detectOpenHandles --verbose --coverage",
    "docs": "rm -r build;  tsc --project ./; jsdoc --configure jsdoc.json --verbose"
  },
